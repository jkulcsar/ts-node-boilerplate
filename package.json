    "spdy": "^4.0.2",
    "swagger-ui-express": "^4.1.4",
    "ts-node": "^8.9.0",
    "tsoa": "^2.5.14",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jest": "^24.9.1",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mongoose": "^5.7.12",
    "@types/mongoose-paginate": "^5.0.8",
    "@types/node": "^12.12.37",
